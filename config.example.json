{
  "activator": "<path-to-virtualenv>/bin/activate_this.py",
  "access_control_allow_origin": "http://localhost:3000",
  "database": {
    "input_page_path": "<path-to-input-page-dir>/output.jsonl",
    "host": "<host>",
    "port": "<port> (NOTE: This value must be an integer. Remove double-quotes.)",
    "db_name": "<db-name>",
    "collection_name": "<collection-name>",
    "log_path": "<path-to-log-dir>/update.log",
    "category_check_log_path": "<path-to-log-dir>/category_checked_pages.jsonl"
  },
  "es": {
    "host": "<host>",
    "port": "<port> (NOTE: This value must be an integer. Remove double-quotes.)"
  },
  "crowdsourcing": {
    "result_dir": "<path-to-result-dir>"
  },
  "feedback": {
    "slack": [
      {
        "access_token": "<oauth-access-token>",
        "channel": "<channel>"
      }
    ],
    "feedback_log_file": "<path-to-feedback-log-file>"
  },
  "source": "<path-to-information-source-list>",
  "password": "<password>"
}
